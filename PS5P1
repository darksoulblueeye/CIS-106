#input phase

qty = float(input("Input quantity of widgets: "))

#process phase

if qty > 10000:
  price = 10
else if qty < 5000:
  price = 30
else:
  price = 20

extprice = qty * price
taxamt = extprice * .07
total = extprice + taxamt

#output phase

print("Subtotal: ", extprice)
print("Tax: ", taxamt)
print("Total: ", total)

